    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "uuid": "9.0.1",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.1.1"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
