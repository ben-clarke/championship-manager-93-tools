    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "13.0.0",
