  "description": "Championship Manager 93 (plus add-ons) editing tools and documentation",
  "private": false,
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "type": "module",
  "scripts": {
